Proceso Ejercicio5
	// Ingresar dos numeros y determinar si son iguales o si el primer numero es menor que el segundo dado su valor .
	Definir n1 , n2 Como Entero;
	Escribir "Ingrese el primer número";
	leer n1;
	Escribir "Ingrese el segundo número";
	leer n2;
	Si n1 == n2 Entonces
		Escribir "El primer número es igual al segundo número";
	SiNo
		Si n1 < n2 Entonces
			Escribir "El número ",n1," es menor que el número ",n2," dado su valor";
		FinSi
	FinSi
FinProceso
